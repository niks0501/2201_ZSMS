<StackPane fx:id="dashCompMainFrame" prefHeight="646.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DashboardCompController">
   <children>
      <AnchorPane prefHeight="646.0" prefWidth="1080.0" style="-fx-background-color: #F5EBE0;">
         <children>
            <Label fx:id="timeLbl" layoutX="14.0" layoutY="53.0" prefHeight="25.0" prefWidth="116.0" text="Time">
               <font>
                  <Font name="Source Sans 3 SemiBold" size="19.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="28.0" fitWidth="31.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../img/time-zone.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
            <Label fx:id="dateLbl" layoutX="14.0" layoutY="92.0" prefHeight="26.0" prefWidth="156.0" text="Date">
               <font>
                  <Font name="Source Sans 3 SemiBold" size="17.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="26.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../img/time-management.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
            <MFXComboBox fx:id="chartMode" floatMode="BORDER" floatingText="Chart Mode" layoutX="14.0" layoutY="14.0" minHeight="0.0" minWidth="0.0" prefHeight="29.0" prefWidth="162.0" />
            <GridPane layoutX="4.0" layoutY="157.0" prefHeight="487.0" prefWidth="1072.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <LineChart fx:id="salesOverview" title="Total Sales">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
                  <BarChart fx:id="topSellingProducts" title="Top Selling Products" GridPane.columnIndex="1">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </BarChart>
                  <AreaChart fx:id="discountEffectiveness" title="Discount Effectiveness" GridPane.rowIndex="1">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </AreaChart>
                  <PieChart fx:id="categoryPerformance" title="Category Performance" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <Label fx:id="totalSalesLbl" alignment="CENTER" contentDisplay="BOTTOM" layoutX="231.0" layoutY="32.0" prefHeight="108.0" prefWidth="156.0" styleClass="summary-field" text="0" wrapText="true">
               <font>
                  <Font name="Inter 18pt ExtraBold" size="25.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="71.0" fitWidth="63.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../img/sale.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
            <Label fx:id="customerWithCreditsLbl" alignment="CENTER" contentDisplay="BOTTOM" layoutX="540.0" layoutY="34.0" prefHeight="68.0" prefWidth="156.0" styleClass="summary-field" text="0">
