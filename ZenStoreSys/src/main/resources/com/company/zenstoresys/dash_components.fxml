                  <Font name="Inter 18pt ExtraBold" size="30.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="64.0" fitWidth="69.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../img/utang.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
            <Label fx:id="totalProductsLbl" alignment="CENTER" contentDisplay="BOTTOM" layoutX="843.0" layoutY="32.0" prefHeight="68.0" prefWidth="156.0" styleClass="summary-field" text="0">
               <font>
                  <Font name="Inter 18pt ExtraBold" size="30.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="64.0" fitWidth="69.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../img/dairyproducts.png" />
